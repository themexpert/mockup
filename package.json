    "jquery": "^3.3.1",
    "popper.js": "^1.14.4",
    "vue": "^2.5.17",
    "vue-color": "^2.6.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "laravel-mix": "^2.1.14",
    "webpack-bundle-analyzer": "^2.13.1"
  }
